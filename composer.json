    "homepage": "http://vespolina-project.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Vespolina Team",
            "homepage": "https://github.com/vespolina/syncer/contributors"
        }
    ],
    "keywords": ["Vespolina", "synchronization", "ecommerce"],
    "require": {
